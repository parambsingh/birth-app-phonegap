<?xml version='1.0' encoding='utf-8'?>
<widget id="portal.myseasonbirthapp.com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NaturalMama</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
        <preference name="Orientation" value="portrait" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#f4cee8" />
    
  <plugin name="cordova-plugin-androidx" source="npm" spec="*" />
  <plugin name="cordova-plugin-androidx-adapter" source="npm" spec="*" />
  
  <plugin name="cordova-plugin-camera" source="npm" spec="*" />
  <plugin name="cordova-plugin-device" source="npm" spec="*" />
  <plugin name="cordova-plugin-file" source="npm" spec="*" />
  <plugin name="cordova-plugin-file-transfer" source="npm" spec="*" />
  
  <plugin name="cordova-plugin-firebase-analytics" source="npm" spec="*" />
  <plugin name="cordova-plugin-firebase-messaging" source="npm" spec="*" />
  
  <plugin name="cordova-plugin-statusbar" source="npm" spec="*" />
  <plugin name="cordova-support-google-services" source="npm" spec="*" />
  
  <plugin name="cordova-support-android-plugin" source="npm" spec="*" />
  <plugin name="cordova-plugin-whitelist" source="npm" spec="*" />
  
	<access origin="*" />
	<access origin="http://portal.myseasonbirthapp.com/api/*" />
	<access origin="http://portal.myseasonbirthapp.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" />
		<icon src="res/icon/android/ldpi.png" density="ldpi" />
        <icon src="res/icon/android/mdpi.png" density="mdpi" />
        <icon src="res/icon/android/hdpi.png" density="hdpi" />
        <icon src="res/icon/android/xhdpi.png" density="xhdpi" />
        <icon src="res/icon/android/xxhdpi.png" density="xxhdpi" />
        <icon src="res/icon/android/xxxhdpi.png" density="xxxhdpi" />
    </platform>
    
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <resource-file src="GoogleService-Info.plist" />
    </platform>
</widget>
